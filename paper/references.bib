@article{ROSENBAUM:1999cq,
author = {Rosenbaum, Paul R},
title = {{Choice as an Alternative to Control in Observational Studies}},
journal = {Statistical Science},
year = {1999},
volume = {14},
number = {3},
pages = {259--278},
month = aug,
annote = {Do:
- Consider a broad theory and find a situation where it could be operating cleanly and with relatively litle interferance
- Define controls intelligently (matching)
- Pick a well-defined treatment that, in a sense, could have happened to anyone
- Define a competing theory, or story
- Multiple treatment assignments leading to the same conclusion are good
- Doses proportional to responses, but undelivered potential doses shouldn't be proportional to responses
-Sensitivity analyses, but also stability analyses (e.g. discrete changes in cohort definition)
- Treatments well-defined in time
- Natural blocks (e.g. twin studies)}
}

